#include <stdio.h>

int main() {
    int n, f;
    printf("Enter number of pages: ");
    scanf("%d", &n);
    int pages[n];
    printf("Enter page reference string: ");
    for(int i=0;i<n;i++)
        scanf("%d", &pages[i]);

    printf("Enter number of frames: ");
    scanf("%d", &f);

    int frame[f], front=0, count=0, pageFaults=0;
    for(int i=0;i<f;i++) frame[i] = -1;

    printf("\nPage\tFrames\t\tFault\n");
    for(int i=0;i<n;i++) {
        int flag=0;
        for(int j=0;j<f;j++)
            if(frame[j]==pages[i]) flag=1;

        if(flag==0) {
            frame[front] = pages[i];
            front = (front+1)%f;
            pageFaults++;
            printf("%d\t", pages[i]);
            for(int j=0;j<f;j++)
                if(frame[j]!=-1) printf("%d ",frame[j]);
                else printf("- ");
            printf("\tPage Fault\n");
        } else {
            printf("%d\t", pages[i]);
            for(int j=0;j<f;j++)
                if(frame[j]!=-1) printf("%d ",frame[j]);
                else printf("- ");
            printf("\tNo Fault\n");
        }
    }
    printf("\nTotal Page Faults = %d\n", pageFaults);
    return 0;
}
